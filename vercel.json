{
  "version": 2,
  "buildCommand": "pnpm install && pnpm --filter=@repo/crypto build && pnpm --filter=api build",
  "installCommand": "pnpm install",
  "framework": null,
  "functions": {
    "apps/api/api/index.ts": {
      "runtime": "@vercel/node@latest"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/api/api/index.ts"
    }
  ]
}