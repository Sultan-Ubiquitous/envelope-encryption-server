{
  "version": 2,
  "buildCommand": "cd ../.. && turbo run build --filter=api...",
  "installCommand": "cd ../.. && npm install",
  "framework": null,
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ],
   "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  }
}